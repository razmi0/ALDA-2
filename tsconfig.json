{
    "extends": "astro/tsconfigs/base",
    "forceConsistentCasingInFileNames": false,
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "exclude": ["node_modules", "dist"],
    "include": [".astro/types.d.ts", "**/*"]
}
