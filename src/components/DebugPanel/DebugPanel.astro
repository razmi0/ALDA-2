---
import type { DebugPanelProps } from "@/types";
import Panel from "./react/DebugPanel";
import "@/components/DebugPanel/react/range.css";

interface Props {
  range: {
    range: DebugPanelProps["range"];
  };
}

const { range } = Astro.props;
const isDev = import.meta.env.DEV;
---

{isDev && <Panel client:only="react" range={range} />}
