---
import { cn } from "@/lib/utils";

interface Props {
    className?: string;
    dataObserver?: string;
    style?: string;
}
const { dataObserver, className, style } = Astro.props;
---

<video loop controls class={cn(className)} data-observer={dataObserver} style={style}>
    <source src={"/catch/catch.mp4"} type="video/mp4" />
</video>
