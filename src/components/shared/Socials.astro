---
import { Facebook as FaceIcon, Instagram as InstaIcon } from "lucide-react";

const links = [
    { href: "https://www.instagram.com/alabordarbre/", label: "instagram", Icon: InstaIcon },
    { href: "https://www.facebook.com/people/A-labordarbre/61564217311513/", label: "facebook", Icon: FaceIcon },
];
---

<ul class="horizontal gap-3 sm:gap-10">
    {
        links.map((l) => {
            const { href, label, Icon } = l;
            return (
                <li>
                    <a href={href} target="_blank">
                        <Icon className={"w-5 h-5 stroke-1"} />
                        <span class="sr-only">{label} link</span>
                    </a>
                </li>
            );
        })
    }
