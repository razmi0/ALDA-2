---
import ClimbIcon from "./svg/climb-icon.svg";
import type { HTMLAttributes } from "astro/types";
import { Image } from "astro:assets";

interface Props {
  className?: HTMLAttributes<"svg">["class"];
}

const { className } = Astro.props;
---

<Image src={ClimbIcon} alt="Tree" class={"-translate-x-[3px] " + className} role={"presentation"} alt="" />
