---
import BirdIcon from "./svg/bird-icon.svg";
import type { HTMLAttributes } from "astro/types";
import { Image } from "astro:assets";

interface Props {
  className?: HTMLAttributes<"svg">["class"];
}

const { className } = Astro.props;
---

<Image src={BirdIcon} alt="Tree" class={"-translate-x-[3px] " + className} role={"presentation"} alt="" />
