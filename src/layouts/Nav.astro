---
import { cn } from "@/lib/utils";

interface Props {
  className?: string;
}

const { className } = Astro.props;

const websiteLinks = [
  { name: "contact", href: "/contact" },
  { name: "nous", href: "/nous" },
  { name: "tarifs", href: "/tarifs" },
  { name: "activités", href: "/activités" },
];
---

<nav class={cn("", className)}>
  {websiteLinks.map(({ name, href }) => <a href={href}>{name}</a>)}
</nav>
